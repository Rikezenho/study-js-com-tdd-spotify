{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "rules": {
    "comma-dangle": "off",
    "no-unused-expressions": "off",
    "arrow-parens": "off",
    "import/prefer-default-export": "off",
    "quotes": ["error", "double"]
  },
  "globals": {
    "describe": true,
    "beforeEach": true,
    "afterEach": true,
    "context": true,
    "it": true
  }
}
